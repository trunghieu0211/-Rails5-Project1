<% if tmp.errors.any? %>
  <div id="error_explanation">
    <div class="alert alert-danger">
      <%= t ".form_contain" %> <%= pluralize tmp.errors.count, t(".error") %>.
    </div>
    <ul>
    <% tmp.errors.full_messages.each do |message| %>
      <li><%= message %></li>
    <% end %>
    </ul>
  </div>
<% end %>
